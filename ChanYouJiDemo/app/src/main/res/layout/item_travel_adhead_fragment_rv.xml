         />
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="@dimen/travel_bacground_image_height"
        android:id="@+id/item_travel_ad_head_second_iv"
        android:scaleType="centerCrop"

    />
    </LinearLayout>


</com.lanou3g.chanyoujidemo.main.view.SlidingAdView>